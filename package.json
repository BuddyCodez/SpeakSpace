    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.7.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.488.0",
    "next": "^15.1.7",
    "next-auth": "npm:next-auth@^4.24.11",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-day-picker": "8.10.1",
