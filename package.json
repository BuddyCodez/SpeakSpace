    "@types/ws": "^8.2.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.1.7",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prisma": "^5.12.1",
    "start-server-and-test": "^1.12.0",
